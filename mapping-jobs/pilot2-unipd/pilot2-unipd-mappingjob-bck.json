{
  "id": "pilot2-unipd-mapping-job",
  "sourceSettings": {
    "source": {
      "jsonClass": "FileSystemSourceSettings",
      "name": "pilot2-source",
      "sourceUri": "https://aiccelerate.eu/unipd",
      "dataFolderPath": "${DATA_FOLDER_PATH}"
    }
  },
  "sinkSettings": {
    "jsonClass": "FhirRepositorySinkSettings",
    "fhirRepoUrl": "${FHIR_REPO_URL}",
    "errorHandling": "continue"
  },
  "mappingErrorHandling": "continue",
  "mappings": [
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2-unipd/patient-mapping",
      "sourceContext": {
        "unipd_export": {
          "jsonClass": "FileSystemSource",
          "path": "unipd-dataset.csv",
          "preprocessSql": "SELECT P.record_id,P.reg_sex,P.patient_reg_death,P.patient_reg_death_age,V.clv_visit_date,V.clv_age_v,PV.npsid_data_npsi,PV.npsid_age_v FROM unipd_export P, unipd_export V, unipd_export PV  WHERE P.record_id = V.record_id AND P.record_id = PV.record_id AND P.redcap_event_name='patient_registry_arm_1' AND V.redcap_event_name='clinical_visit_arm_1' AND V.redcap_repeat_instance = 1 AND PV.redcap_event_name='npsi_arm_1' AND PV.redcap_repeat_instance = 1",
          "options": {
            "sep": ";",
            "ignoreLeadingWhiteSpace": "true",
            "ignoreTrailingWhiteSpace": "true"
          }
        }
      }
    },
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2-unipd/encounter-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "unipd-dataset.csv",
          "options": {
            "sep": ";",
            "ignoreLeadingWhiteSpace": "true",
            "ignoreTrailingWhiteSpace": "true"
          }
        }
      }
    },
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2-unipd/condition-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "unipd-dataset.csv",
          "options": {
            "sep": ";",
            "ignoreLeadingWhiteSpace": "true",
            "ignoreTrailingWhiteSpace": "true"
          }
        }
      }
    },
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2-unipd/symptom-existence-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "unipd-dataset.csv",
          "options": {
            "sep": ";",
            "ignoreLeadingWhiteSpace": "true",
            "ignoreTrailingWhiteSpace": "true"
          }
        }
      }
    },
    {
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2-unipd/medication-used-mapping",
      "sourceContext": {
        "source": {
          "jsonClass": "FileSystemSource",
