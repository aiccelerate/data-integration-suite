{
  "id": "pilot2-mappingjob",
  "sourceSettings": {
    "source": {
      "jsonClass": "FileSystemSourceSettings",
      "name": "pilot2-source",
      "sourceUri": "https://aiccelerate.eu/data-integration-suite/pilot2-data",
      "dataFolderPath": "test-data/pilot2-r"
    }
  },
  "sinkSettings": {
    "jsonClass": "FhirRepositorySinkSettings",
    "fhirRepoUrl": "${FHIR_REPO_URL}"
  },
  "mappings": [
    {
      "name": "patient-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/patient-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "patients.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "encounter-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/encounter-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "encounters.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "condition-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/condition-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path": "conditions-withoutCodeDisplay.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "medication-used-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/medication-used-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path":  "medication-used.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "device-used-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/device-used-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path":  "device-used.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "symptom-existence-mapping",
