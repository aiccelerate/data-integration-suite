      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path":  "other-assessments.csv",
          "contentType": "csv",
          "sourceRef": "source"
        }
      }
    },
    {
      "name": "clinical-note-mapping",
      "mappingRef": "https://aiccelerate.eu/fhir/mappings/pilot2/clinical-note-mapping",
      "sourceBinding": {
        "source": {
          "jsonClass": "FileSystemSource",
          "path":  "clinical-notes.csv",
