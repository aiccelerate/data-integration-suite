{
  "url": "https://aiccelerate.eu/fhir/mappings/radiological-studies-mapping",
  "name": "radiological-studies-mapping",
  "title": "Mapping of radiological studies schema for pilots to AIC-DiagnosticStudy FHIR profile",
  "source": [{
    "alias": "source",
    "url": "/StructureDefinition/ext-radiological-studies"
  }],
  "mapping": [
    {
      "expression": {
        "name": "result",
        "language": "application/fhir-template+json",
        "value": {
          "resourceType": "DiagnosticReport",
          "id": "{{utl:uuid()}}",
          "status": "final",
          "category": [
            {
              "coding": [
                {
                  "system": "http://terminology.hl7.org/CodeSystem/v2-0074",
                  "code": "RAD",
                  "display": "Radiology"
                }
              ]
            }
          ],
          "code": {
            "coding": [
              {
                "system": "https://koodistopalvelu.kanta.fi/codeserver/pages/classification-view-page.xhtml?classificationKey=57&versionKey=119",
                "code": "{{code}}",
                "display": "{{codeDisplay}}"
              }
            ]
          },
          "subject": {
            "reference": "Patient/{{pid}}"
          },
          "effectiveDateTime": "{{time}}",
          "encounter": "{{? utl:createFhirReference('Encounter', encounterId)}}"
        }
      }
    }
  ]
}