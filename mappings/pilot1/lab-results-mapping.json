{
  "url": "https://aiccelerate.eu/fhir/mappings/lab-results-mapping",
  "name": "lab-results-mapping",
  "title": "Mapping of schema lab results to AIC-LabResults profile",
  "source": [{
    "alias": "source",
    "url": "/StructureDefinition/ext-lab-results"
  }],
  "context": {
    "labResultsConceptMap":  {
      "category": "concept-map",
      "url": "./lab-results-concept-map.csv"
    }
  },
  "mapping": [
    {
      "expression": {
        "name": "result",
        "language": "application/fhir-template+json",
        "value": {
          "resourceType": "Observation",
          "id": "{{utl:uuid()}}",
          "meta": {
            "profile": ["https://aiccelerate.eu/fhir/StructureDefinition/AIC-LabResultWithinSurgicalWorkflow"]
          },
          "status": "final",
          "category": [
            {
              "coding": [{
                "system": "http://terminology.hl7.org/CodeSystem/observation-category",
                "code": "laboratory",
                "display": "Laboratory result"
              }]
            }
          ],
          "code": {
            "coding": [
              {
                "system": "{{mpp:getConcept(%labResultsConceptMap, code, 'source_system')}",
                "code": "{{code}}",
                "display": "{{description}}"
              },
              {
                "{{#tc}}": "{{mpp:getConcept(%labResultsConceptMap, code, 'target_code')}",
                "{{?}}": {
                  "system": "http://loinc.org",
                  "code": "{{%tc}}",
                  "display": "{{mpp:getConcept(%labResultsConceptMap, code, 'target_display')}}"
                }
              }
            ]
          },
          "subject": {
            "reference": "Patient/{{pid}}"
          },
          "encounter": "{{? utl:createFhirReference('Encounter', encounterId)}}",
          "effectiveDateTime": "{{time}}",
          "valueQuantity": {
            "value": "{{value}}",
            "unit": "{{mpp:getConcept(%vitalSignsConceptMap, code, 'unit')}}",
            "system": "http://unitsofmeasure.org",
            "code": "{{mpp:getConcept(%vitalSignsConceptMap, code, 'unit')}}"
          }
        }
      }
    }
  ]
}